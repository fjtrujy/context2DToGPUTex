{
  "name": "webgpu",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:github": "GITHUB_PAGES=true tsc && GITHUB_PAGES=true vite build",
    "preview": "vite preview",
    "serve": "serve -s dist -l 3000",
